#kkrpc模块springboot配置模版
#application相关配置
melot.kkrpc.applicationConfig.appName=demo
melot.kkrpc.applicationConfig.author=xxx
melot.kkrpc.applicationConfig.appVersion=1.0.0

#kkrpc注册中心相关配置
melot.kkrpc.registryConfig.address=zk1.kktv2.com:2181
melot.kkrpc.registryConfig.sessionTimeout=10000
melot.kkrpc.registryConfig.sessconnTimeout=30000

#kkrpc服务提供方配置
melot.kkrpc.serviceConfig[0].interfaceName=com.melot.kkrpc.api.MyRobotService
melot.kkrpc.serviceConfig[0].port=30001
melot.kkrpc.serviceConfig[0].version=1.0.1
melot.kkrpc.serviceConfig[0].methods[0].name=getDate

#kkrpc服务消费方配置
melot.kkrpc.referenceConfig[0].interfaceName=com.melot.kkrpc.api.MyRobotService
melot.kkrpc.referenceConfig[0].version=1.0.1
melot.kkrpc.referenceConfig[0].method[0].name=getDate
